<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CommonBackgroundStyle">
        <attr name="background_radius" format="dimension" />
        <attr name="deviation" format="enum">
            <enum name="VERTICAL" value="0" />
            <enum name="HORIZONTAL" value="1" />
            <enum name="SLANTING_DOWN" value="2" />
            <enum name="SLANTING_TOP" value="3" />
        </attr>

        <attr name="stroke_width" format="dimension" />
        <attr name="stroke_colors" format="string" />
        <attr name="stroke_deviation" format="enum">
            <enum name="VERTICAL" value="0" />
            <enum name="HORIZONTAL" value="1" />
            <enum name="SLANTING_DOWN" value="2" />
            <enum name="SLANTING_TOP" value="3" />
        </attr>

        <attr name="background_colors" format="string" />
        <attr name="background_deviation" format="enum">
            <enum name="VERTICAL" value="0" />
            <enum name="HORIZONTAL" value="1" />
            <enum name="SLANTING_DOWN" value="2" />
            <enum name="SLANTING_TOP" value="3" />
        </attr>
    </declare-styleable>
</resources>